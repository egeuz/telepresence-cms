{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "Event"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "speaker": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "event_type": {
      "type": "string"
    },
    "eventbrite_url": {
      "type": "string"
    },
    "speaker_social": {
      "type": "dynamiczone",
      "components": [
        "text.portfolio",
        "text.facebook",
        "text.instagram",
        "text.linked-in",
        "text.email"
      ]
    },
    "date": {
      "type": "string"
    },
    "time": {
      "type": "string"
    }
  }
}
